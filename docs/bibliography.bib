@article{en14185893,
	abstract = {Diagnostics of power and energy systems is obviously an important matter. In this paper we present a contribution of using new methodology for the purpose of signal type recognition (for example, faulty/healthy or different types of faults). Our approach uses Bayesian functional data analysis with data depths distributions to detect differing signals. We present our approach for discrimination of pole-to-pole and pole-to-ground short circuits in VSC DC cables. We provide a detailed case study with Monte Carlo analysis. Our results show potential for applications in diagnostics under uncertainty.},
	article-number = {5893},
	author = {Baranowski, Jerzy and Grobler-D{\k e}bska, Katarzyna and Kucharska, Edyta},
